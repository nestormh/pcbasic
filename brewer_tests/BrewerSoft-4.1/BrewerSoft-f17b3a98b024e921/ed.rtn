16050                       QC=QC+1:G$(QC)="u"+"vsum"
16060 IF TYP$="mkiv" THEN   QC=QC+1:G$(QC)="n"+"osum"
16070 IF TYP$="mkiv" THEN   QC=QC+1:G$(QC)="z"+"ssum"
16080                       QC=QC+1:G$(QC)="f"+"msum"
